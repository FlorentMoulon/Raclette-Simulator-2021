<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="raclette simulator" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Grapic">
				<Option output="../../bin/raclette simulator.exe" prefix_auto="0" extension_auto="0" />
				<Option object_output="obj/Grapic/raclette simulator" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-DWIN32" />
					<Add option="-DNVWIDGETS_EXPORTS" />
					<Add option="-D_USE_MATH_DEFINES" />
					<Add option="-D_CRT_SECURE_NO_WARNINGS" />
					<Add option="-DNOMINMAX" />
					<Add option="-std=c++11" />
					<Add option="-g" />
					<Add option="-W -Wall -Wextra -Wno-sign-compare -Wno-unused-parameter -Wno-unused-function -Wno-unused-variable -Wno-comment -Wno-unused-but-set-variable -Wno-narrowing" />
					<Add directory="../.." />
					<Add directory="../../src" />
					<Add directory="../../extern/mingw-cb20/include" />
					<Add directory="../../extern/mingw-cb20/include/SDL2" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add option="-g" />
					<Add directory="../../extern/mingw-cb20/lib" />
					<Add directory="../../extern/mingw-cb20/bin" />
					<Add directory="." />
					<Add library="mingw32" />
					<Add library="SDL2main" />
					<Add library="SDL2" />
					<Add library="SDL2_image" />
					<Add library="SDL2_ttf" />
				</Linker>
			</Target>
		</Build>
		<Unit filename="../../src/Grapic.cpp">
			<Option virtualFolder="src" />
		</Unit>
		<Unit filename="../../src/Grapic.h">
			<Option virtualFolder="src" />
		</Unit>
		<Unit filename="../../apps/raclette simulator/raclette simulator.cpp">
			<Option virtualFolder="apps/raclette simulator" />
		</Unit>
		<Extensions>
		</Extensions>
	</Project>
</CodeBlocks_project_file>

